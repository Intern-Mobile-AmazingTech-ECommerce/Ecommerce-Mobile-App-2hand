<?xml version="1.0" encoding="utf-8"?>
<resources>

   

    <style name="font_circular_bold">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">#272727</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="font_circular">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">#272727</item>
    </style>
    <style name="btn_continue">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>


    <style name="clear">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#272727</item>
    </style>

    <style name="overlay_title">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#272727</item>
    </style>

    <style name="user_name">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#272727</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/gabarito</item>
    </style>

    <style name="user_email">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#802727</item>
        <item name="fontFamily">@font/circularstdmedium500</item>
    </style>

    <style name="user_phonenumber">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#802727</item>
        <item name="fontFamily">@font/circularstdmedium500</item>
    </style>
    <style name="edit">
        <item name="android:text">Edit</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#8E6CEF</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/gabarito</item>
        <item name="lineHeight">14sp</item>
    </style>

    <style name="setting_items">
        <item name="android:textColor">#272727</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@drawable/frame_59</item>
        <item name="android:drawableEnd">@drawable/arrowright2</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="fontFamily">@font/circularstdmedium500</item>
    </style>
    <style name="setting_items_label">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#272727</item>
        <item name="fontFamily">@font/gabarito</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="address_item">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#272727</item>
        <item name="fontFamily">@font/circularstdmedium500</item>
    </style>
    <style name="sign_out">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#FA3636</item>
        <item name="fontFamily">@font/gabarito</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>